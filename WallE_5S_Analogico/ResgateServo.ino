void MovimentoServo() {

  Servo2.write(105); //braço direito para cima
  delay(500);
  Servo2.write(90); //braço direito para cima
  delay(500);
  Servo2.write(70); //braço direito para cima
  delay(500);
  Servo2.write(60); //braço direito para cima
  delay(500);
  Servo2.write(50); //braço direito reto
  delay(15000);

  Servo2.write(60); //braço direito para cima
  delay(500);
  Servo2.write(70); //braço direito para cima
  delay(500);
  Servo2.write(90); //braço direito para cima
  delay(500);
  Servo2.write(105); //braço direito para cima
  delay(500);
}
